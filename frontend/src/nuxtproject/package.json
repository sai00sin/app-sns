{
  "name": "nuxtproject",
  "version": "1.0.0",
  "description": "My sublime Nuxt.js project",
  "author": "Saishin Inoue",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "test": "jest"
  },
  "dependencies": {
    "nuxt": "^2.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".*\\.(vue)$": "vue-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/app/$1",
      "^@@/(.*)$": "<rootDir>/$1",
      "^~/(.*)$": "<rootDir>/app/$1",
      "^~~/(.*)$": "<rootDir>/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "collectCoverageFrom": [
      "app/**/*.{js,vue}"
    ]
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          "@babel/preset-env"
        ]
      }
    }
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@vue/test-utils": "^1.0.0-beta.32",
    "babel-core": "^6.26.3",
    "babel-jest": "^25.2.1",
    "jest": "^25.2.0",
    "lodash.clonedeep": "^4.5.0",
    "vue-jest": "^3.0.5"
  }
}
